def selectionSort(a):
    for i in range(len(a)-1, 0, -1):
        positie = i
        max = a[i]
        for j in range((i-1),-1, -1):
            if a[j] > max:
                positie = j
                max = a[j]

        a[positie] = a[i]
        a[i] = max


arr = [44,55,12,42,94,18,6,67]
selectionSort(arr)
print(arr)